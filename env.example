#
# Event Conflict Finder Environment Template
#

# Server configuration
PORT=3000
NODE_ENV=development

# Ticketing APIs
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here
TICKETMASTER_ENABLED=true
BANDSINTOWN_APP_ID=EventConflictFinder
BANDSINTOWN_ENABLED=true

# Redis cache / rate limiting
REDIS_URL=redis://localhost:6379
CACHE_TTL_SECONDS=900

# Supabase (premium paywall)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_PLAN_TABLE=user_plans
FREE_SEARCH_LIMIT=3

# Polar payments
POLAR_API_BASE_URL=https://api.polar.sh/v1
POLAR_API_KEY=your_polar_api_key
POLAR_PRODUCT_ID=your_polar_product_id
POLAR_PRODUCT_PRICE_ID=your_polar_product_price_id
POLAR_PAYMENT_PROCESSOR=stripe
POLAR_SUCCESS_URL=http://localhost:3000/?payment=success
POLAR_CANCEL_URL=http://localhost:3000/?payment=cancelled
POLAR_WEBHOOK_SECRET=your_polar_webhook_secret
FRONTEND_URL=http://localhost:3000

# Monitoring
ENABLE_MONITORING=true
MONITORING_LOG_INTERVAL=300000
MONITORING_PERF_HISTORY=25

