# Event Conflict Finder - Environment Variables
# Copy this file to .env and fill in your actual API keys

# ============================================
# Eventbrite API Configuration
# ============================================
# Get your API key from: https://www.eventbrite.com/platform/api-keys/
# Note: Eventbrite uses OAuth tokens, not simple API keys
# Format: 20-60 alphanumeric characters
# Example: CPXUFKMXTKCJIY5MFN (20 chars) or longer OAuth token (40+ chars)
EVENTBRITE_API_KEY=B3TJU6YWDRCDXZSDXTSWMVPL6CUCTS3Z244VTRUXCWUFZRHHPP

# Enable/disable Eventbrite service (true/false, default: true)
EVENTBRITE_ENABLED=true

# Eventbrite API base URL (optional, uses default if not set)
# EVENTBRITE_BASE_URL=https://www.eventbriteapi.com/v3

# Request timeout in milliseconds (default: 10000 = 10 seconds)
EVENTBRITE_TIMEOUT=10000

# Maximum number of results to fetch (default: 50)
EVENTBRITE_MAX_RESULTS=50

# ============================================
# Ticketmaster API Configuration
# ============================================
# Get your API key from: https://developer.ticketmaster.com/
# Format: 30-50 mixed case alphanumeric characters
# Example: QaRILBv3HxGRfoC2PfXMIS1CDl61MDyb (32 chars)
TICKETMASTER_API_KEY=QaRILBv3HxGRfoC2PfXMIS1CDl61MDyb

# ============================================
# Ticket Tailor API Configuration
# ============================================
# Get your API key from: https://www.tickettailor.com/developers/api
# Format: sk_ followed by numbers, underscore, and hex string (40-60 chars total)
# Example: sk_10598_277807_817291d925c85e7300111c5d850dff5a (47 chars)
TICKETTAILOR_API_KEY=sk_10598_277807_817291d925c85e7300111c5d850dff5a

# Enable/disable Ticket Tailor service (true/false, default: true)
TICKETTAILOR_ENABLED=true

# Ticket Tailor API base URL (optional, uses default if not set)
# TICKETTAILOR_BASE_URL=https://api.tickettailor.com/v1

# Request timeout in milliseconds (default: 10000 = 10 seconds)
TICKETTAILOR_TIMEOUT=10000

# Authentication type: 'bearer' or 'basic' (default: bearer)
# If you get 403 errors, try switching to 'basic'
TICKETTAILOR_AUTH_TYPE=basic

# ============================================
# Server Configuration
# ============================================
# Server Port (optional, defaults to 3000)
PORT=3000

# Node Environment (optional)
NODE_ENV=development

#========================
